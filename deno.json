{
  "importMap": "./import_map.json",
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ --unstable dev.ts",
    "bench": "deno bench bench.ts",
    "build:css": "deno run --allow-env --allow-read --allow-write build.css.ts",
    "fmt": "deno fmt",
    "esm:add": "deno run -A https://esm.sh/v124 add",
    "esm:update": "deno run -A https://esm.sh/v124 update",
    "esm:remove": "deno run -A https://esm.sh/v124 remove"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
