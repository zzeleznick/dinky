{
  "importMap": "./import_map.json",
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ --unstable dev.ts",
    "bench": "deno bench bench.ts",
    "build:css": "deno run --allow-env --allow-read --allow-write build.css.ts",
    "serve": "deno run --unstable --allow-env --allow-read --allow-net --allow-write --allow-ffi main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}